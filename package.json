{
    "name": "json-visualizer",
    "private": true,
    "version": "1.0.2",
    "description": "Interactive JSON data visualization and manipulation tool",
    "type": "module",
    "engines": {
        "node": ">=22.0.0",
        "npm": ">=10.0.0"
    },
    "scripts": {
        "dev": "vite --host",
        "build": "vue-tsc && vite build",
        "preview": "vite preview",
        "type-check": "vue-tsc --noEmit",
        "release": "npm run build && npm version patch && git push && git push --tags",
        "release:minor": "npm run build && npm version minor && git push && git push --tags",
        "release:major": "npm run build && npm version major && git push && git push --tags"
    },
    "dependencies": {
        "@guolao/vue-monaco-editor": "^1.5.5",
        "@ltd/j-toml": "^1.38.0",
        "@vueuse/core": "^10.7.2",
        "ajv": "^8.12.0",
        "d3": "^7.8.5",
        "file-saver": "^2.0.5",
        "html2canvas": "^1.4.1",
        "jq-web": "^0.5.1",
        "js-yaml": "^4.1.0",
        "jsonpath-plus": "^10.3.0",
        "jsonwebtoken": "^9.0.2",
        "lodash-es": "^4.17.21",
        "papaparse": "^5.4.1",
        "pinia": "^2.1.7",
        "prismjs": "^1.29.0",
        "vue": "^3.4.15",
        "vue-prism-editor": "^2.0.0-alpha.2",
        "xml2js": "^0.6.2"
    },
    "devDependencies": {
        "@iconify/vue": "^4.1.1",
        "@types/d3": "^7.4.3",
        "@types/file-saver": "^2.0.7",
        "@types/js-yaml": "^4.0.9",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^20.11.17",
        "@types/papaparse": "^5.3.14",
        "@types/prismjs": "^1.26.5",
        "@types/xml2js": "^0.4.14",
        "@vitejs/plugin-vue": "^6.0.0",
        "sass": "^1.89.2",
        "typescript": "^5.2.2",
        "vite": "^7.0.4",
        "vue-tsc": "^3.0.1"
    }
}
